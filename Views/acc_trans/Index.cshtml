@model IEnumerable<IgorMarinho_WebApp.acc_trans>
@*@model IgorMarinho_WebApp.Accounts*@

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.trans_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.acc_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.trans_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.trans_value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.trans_reg_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.trans_rem_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.repeat)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.trans_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.acc_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.trans_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.trans_value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.trans_reg_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.trans_rem_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.repeat)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.trans_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.trans_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.trans_id })
            </td>
        </tr>
    }

</table>
