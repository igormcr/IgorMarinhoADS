@model IEnumerable<IgorMarinho_WebApp.Accounts>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.acc_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.acc_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ins_total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.outs_total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user_id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.acc_id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.acc_name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ins_total)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.outs_total)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.user_id)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.acc_id }) |
        @Html.ActionLink("Details", "Details", new { id = item.acc_id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.acc_id })
    </td>
</tr>
}

</table>
